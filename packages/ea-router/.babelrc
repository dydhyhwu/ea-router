{
  "env": {
    "dev": {
      "plugins": [
        ["@babel/plugin-proposal-decorators", {
          "legacy": true
        }],
        ["@babel/plugin-proposal-class-properties", {"loose": true}]
      ]
    },
    "build": {
      "presets": ["@babel/preset-env"],
      "plugins": [
        "transform-class-properties"
      ]
    }
  }
}
